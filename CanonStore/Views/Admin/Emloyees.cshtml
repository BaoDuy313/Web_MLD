@model List<CanonStore.Models.Emloyee>
@{
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<div class="main">
    <h1>Emloyees</h1><br />
    <a href="~/Admin/EmloyeeAdd"><h3 style="color:Highlight;">New Emloyee</h3></a><br /><br />
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Phone</th>
                <th scope="col">User Name</th>
                <th scope="col">Password</th>
                <th scope="col">Action</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.Name</td>
                    <td>@item.Phone</td>
                    <td>@item.UserName</td>
                    <td>@item.Password</td>
                    <td>
                        <a href="~/Admin/EmloyeeEdit/@item.Id">Edit</a> |
                        <a href="~/Admin/EmloyeeDelete/@item.Id">Delete</a>
                    </td>

                </tr>
            }

        </tbody>
    </table>
</div>
